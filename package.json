{
  "name": "dmd-jc",
  "author": "Javier Caballero <caballerojavier13@gmail.com>",
  "version": "4.0.7",
  "description": "The default output template for jsdoc-to-markdown",
  "license": "MIT",
  "repository": "https://github.com/caballerojavier13/dmd",
  "keywords": [
    "documentation",
    "markdown"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "index.js",
    "helpers/*.js",
    "lib/*.js",
    "partials"
  ],
  "scripts": {
    "test": "test-runner test/*.js test/ddata/*.js"
  },
  "dependencies": {
    "array-back": "^4.0.0",
    "cache-point": "^1.0.0",
    "common-sequence": "^2.0.0",
    "file-set": "^3.0.0",
    "handlebars": "^4.5.1",
    "marked": "^4.0.10",
    "object-get": "^2.1.0",
    "reduce-flatten": "^3.0.0",
    "reduce-unique": "^2.0.1",
    "reduce-without": "^1.0.1",
    "test-value": "^3.0.0",
    "walk-back": "^4.0.0"
  },
  "devDependencies": {
    "dmd-plugin-example": "^0.1.0",
    "nyc": "^14.1.1",
    "test-runner": "^0.7.2"
  },
  "standard": {
    "ignore": [
      "example",
      "test/fixture",
      "test/overrides"
    ]
  }
}
